{
    "name": "robertogallea/laravel-ui-faces",
    "description": "Laravel package wrapping UI Faces API",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Roberto Gallea",
            "email": "write@robertogallea.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "robertogallea\\UIFaces\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/support": "^5.8.3|^6.0.0",
        "guzzlehttp/guzzle": "~6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2"
    },
    "extra": {
    "laravel": {
        "providers": [
            "robertogallea\\UIFaces\\UIFacesServiceProvider"
        ]
    }
}
}
